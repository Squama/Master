            <![CDATA[
            select jys.* from (
              SELECT jy.* ,(select le.name from tbl_worker wk,tbl_worker le where wk.id = jy.workerid and wk.leaderid = le.id) leaname
			  FROM tbl_aqjy jy ) jys 
			  WHERE 1=1 
