        <column key="status" header="状态" allowSort="true" width="150" render="10=新建,20=负责人审批,30=办公室主任审批,40=总经理审批,50=财务审批,60=已完成,70=审核不通过"/>
        <column key="id" header="操作" width="80" fnRender="fnRenderSalary"/>
